import br.com.bio.model.Cat;
public class First Program {
    public static void main(String[] args) {
        Book book = new Book();
        Cat cat = new Cat();

        System.out.println(cat);
        System.out.println(book);
    }
}

class Book {
    public String book;
    public integer numPages;
}
