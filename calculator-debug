package br.com.bio. calculator;

public class Calculator {
    public static void main (String[] args){
        Scanner scan = new Scanner(System.in);

        int a, b;

        System.out.println("Insert the first value:")
        a = scan.nextInt();
        System.out.println("Insert the second value:")
        b = scan.nextInt();

        add (a, b);                                //ctrl 1 assign statemente the new variable
        int add = add(a,b)

        int add = add(a,b);
        int subtraction = subtraction (a,b);         //ctrl 2 l
        int multiplication = multiplication (a,b);
        float divide = divide (a,b); 
        
        System.out.println("add" + add );
         System.out.println(s"sub"+ subtraction);
          System.out.println("div"+ divide);
           System.out.println("mult" + multiplication);




    }
    public static int add(int a,b) {
        return a+b;
    }
    public static int subtraction(int a,b) {
        return a - b;
    }
    public static int divide (int a,b) {
        return a / b;
    }
    public static int multiple(int a,b) {
        return a * b;
    }    
}


